{
  "name": "mitrais-coding-test",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:reset": "npx sequelize-cli db:seed:undo:all && npm run seed",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dendyramdhan/mitrais-coding-test.git"
  },
  "keywords": [
    "Coding",
    "Test"
  ],
  "author": "Dendy Ramdhan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dendyramdhan/mitrais-coding-test/issues"
  },
  "homepage": "https://github.com/dendyramdhan/mitrais-coding-test#readme",
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-validator": "^6.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
